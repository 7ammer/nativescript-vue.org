<div class="bg-grey-lighter md:hidden">
    <div class="container mx-auto p-4">
        <select v-nav class="w-full p-2" title="<%= title %>">
            <% items.forEach(item => { %>
            <option value="/<%= item.path %>" <%= current.path === item.path ? 'selected' : '' %>><%= item.title %></option>
            <% }) %>
        </select>
    </div>
</div>